    "babel-runtime": "^6.6.1",
    "bootstrap": "^3.3.6",
    "bugsnag-js": "^2.5.0",
    "hapi-sdk": "holidayextras/hapi-sdk#3.1.0",
    "js-cookie": "^2.1.0",
    "lodash": "^4.5.0",
    "moment": "^2.11.2",
    "qs": "^6.1.0",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "superagent": "^1.7.2"
