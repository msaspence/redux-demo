{
  "name": "trip-planner",
  "version": "0.0.1",
  "description": "The JavaScript application behind HEHA!",
  "main": "./dist/javascript/index.js",
  "keywords": [
    "heha",
    "trip",
    "planner"
  ],
  "author": "Holiday Extras, HEHA Pod",
  "contributors": [
    "Jack Crawford <jack.crawford@holidayextras.com>",
    "Matthew Spence <matthew.spence@holidayextras.com>",
    "Ollie Buck <ollie.buck@holidayextras.com>",
    "Joe Warren <joe.warren@holidayextras.com>"
  ],
  "repository": "holidayextras/trip-planner",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "port": 3060,
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "engines": {
    "node": "4.2.3"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:static": "cpy 'src/**/*.html' dist",
    "build:js": "webpack --config webpack.config.js",
    "start": "npm-run-all --parallel start:*",
    "start:js": "webpack-dev-server --content-base dist/ --inline --port $npm_package_config_port",
    "start:static": "watchy -w ./src/**/*.html -- npm run build:static",
    "start:less": "watchy -w ./src/stylesheets/**/* -- npm run build:less"
  },
  "dependencies": {
    "@holidayextras/brand-globals": "^1.0.3",
