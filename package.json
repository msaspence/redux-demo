    "babel-istanbul-loader": "0.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "cheerio": "^0.20.0",
    "coveralls": "^2.11.6",
    "cpy-cli": "^1.0.0",
    "create-thenable": "^1.0.1",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.2.0",
