    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-osx-reporter": "^0.2.1",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "less": "^2.6.0",
    "make-up": "^8.2.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1",
